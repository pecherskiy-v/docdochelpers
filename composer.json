{
  "name": "pecherskiy-v/docdochelpers",
  "description": "Simple helper for use docdoc.ru partner API",
  "keywords": [
    "docdoc"
  ],
  "version": "2.1.1",
  "homepage": "https://github.com/Leyhmann/docdochelpers",
  "license": "MIT",
  "authors": [
    {
      "name": "Leyhmann Andrey",
      "email": "leyhmann.a.a@gmail.com"
    },
    {
      "name": "Pecherskiy Valeriy",
      "email": "pecherskiy.v@gmail.com",
      "homepage": "https://github.com/pecherskiy-v/"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "nesbot/carbon": "2.*",
    "kriswallsmith/buzz": "^0.17.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Pecherskiy\\DocDoc\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pecherskiy\\DocDoc\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit tests"
  },
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
